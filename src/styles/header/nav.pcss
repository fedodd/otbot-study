.menu {
	margin: 0;
	padding: 0;
	width: 100%;
}

.nav {
	display: flex;
	align-items: center;
	margin: 0;
	padding: 0;
	list-style: none;
	font-weight: bold;
}


.nav-item {
	&.is__alt {
		margin-left: auto;
	}
}

.nav-link {
	display: block;
	margin: 0 10px;
	padding: 5px 10px;
	text-decoration: none;
	color: white;
}
